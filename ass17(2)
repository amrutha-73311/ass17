#include<stdio.h>
#include<conio.h>
void main()
{
int i,n,a,b,num;
a=b=0;
clrscr();
printf("enter the maximum amount of numbers::");
scanf("%d",&n);
printf("\n\n");
for(i=0;i<n;i++);
{
printf("\n enter the number");
scanf("%d",&num);
if(num>a)
{
b=a;
a=num;
}
else if(num>b)
b=num;
}
printf("\n\n second highest number is%d",b);
getch();
}
